with defender {
    bees = workers
    other.workers -= bees
    home_x = x
    home_y = y
    state = "combat"
    order_state = "return"
    with instance_nearest(x,y,obj_hive_combat) {
    units_total++
    units[0].defender_num = units_total
    units[units_total] = other.id }
}
}

if (defender != "none") {
defender.bees++
workers--
}

}
