        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Gain Royal Jelly

// Heal queen

while (hp &lt; max_hp &amp;&amp; drones &gt; 0) {
    drones--
    hp += 5
    if hp &gt; max_hp {hp = max_hp}
}

// Make jelly

jelly_progress += ceil((hp/max_hp)*100)

if jelly_progress &gt;= 100 {
    // Increase jelly
    self.rpc_id = htme_hash()
    var playerlist = htme_getPlayers()
    list_p1 = ds_list_create()
    ds_list_add(list_p1,x,y,300,400)
    for(var i = 0;i&lt;ds_list_size(playerlist);i++) {
        var player = ds_list_find_value(playerlist,i)
        htmerpc_send(self.rpc_id,scr_rpc_royaljelly,player,p_id)
    }
    // Jelly carryover
    jelly_progress -= 100
    jelly_progress /= 4
    jelly_progress = floor(jelly_progress)
}
</string>
          </argument>
        </arguments>
      </action>
