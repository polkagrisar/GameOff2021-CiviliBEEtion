            <string>///If a bee returns mid-combat
if (state == "combat" &amp;&amp; workers &gt; 0) {

if (defender == "none") {
defender = instance_create(x,y,obj_bee)
with defender {
    bees = other.workers
    type = "defend"
    p_id = other.p_id
    other.workers -= bees
    home_x = x
    home_y = y
    state = "combat"
    order_state = "return"
    with instance_nearest(x,y,obj_hive_combat) {
    units[1] = other.id}
}

if (defender != "none") {
defender.bees++
workers--}

}

}
