            <string>///Left Click

//Check range 
//if (within range of hive + range upgrade) (Encompass the whole left-click script)

if (mouse_check_button_pressed(mb_left)) {
    if (hover != "none") {
        
        //If space
        if (hover.type == "space") { 
            target = hover
            state = "orders"
        
            with hover {
                if (order == "none") {
                    spawn_x = other.owner.x
                    spawn_y = other.owner.y
                    user = other.id
                    order = instance_create(x,y,obj_order)
                    with order {creator = other.id
                    user = other.user
                    spawn_x = other.spawn_x
                    spawn_y = other.spawn_y
                    p_id = user.p_id}
                }
            }
        }
        
        //If opponent hive
        if (hover.type == "hive" &amp;&amp; hover.p_id != p_id) {
        target = hover
        state = "orders"
        
        with hover {
                if (order == "none") {
                    spawn_x = other.owner.x
                    spawn_y = other.owner.y
                    user = other.id
                    order = instance_create(x,y,obj_order)
                    with order {creator = other.id
                    user = other.user
                    spawn_x = other.spawn_x
                    spawn_y = other.spawn_y}
                }
            }
        
        }
        
        //If empty space
        if (hover.type == "nospace") { 
         target = hover
         state = "orders"}
        
        // if order
        if (hover.type == "order") {with hover {scr_order("add")} }
    
    }

}

//Press Empty
if (mouse_check_button_pressed(mb_left) &amp;&amp; hover == "none") {

    //Send patrol to empty area
    hover = instance_create(x,y,obj_nospace)
    target = hover
    state = "orders"
    with hover {
                if (order == "none") {
                    user = other.id
                    spawn_x = other.owner.x
                    spawn_y = other.owner.y
                    order = instance_create(x,y,obj_order)
                    with order {creator = other.id
                    user = other.user
                    spawn_x = other.spawn_x
                    spawn_y = other.spawn_y
                    p_id = user.p_id}
                }
            }
}
</string>
